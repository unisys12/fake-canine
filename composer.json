{
    "name": "unisys12/fake-canine",
    "description": "Extend FakerPHP to add support for adding fake Canines to a project",
    "keywords": [
        "unisys12",
        "fake-canine"
    ],
    "homepage": "https://github.com/unisys12/fake-canine",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Phillip Jackson",
            "email": "unisys12@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "fakerphp/faker": "^1.13"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.64",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Unisys12\\FakeCanine\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Unisys12\\FakeCanine\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-w": "vendor\\bin\\phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "test-coverage-w": "vendor\\bin\\phpunit --coverage-html coverage",
        "analyse": "vendor/bin/phpstan",
        "analyse-w": "vendor\\bin\\phpstan analyse --level 5 src tests"
    },
    "config": {
        "sort-packages": true
    }
}
